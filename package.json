{
  "name": "ionic2-webpack-starter",
  "version": "0.1.0",
  "description": "An Ionic 2 starter kit that uses Webpack with HMR, Karma, Protractor, Istanbul, Typescript, Typings, TSLint and Codelyzer.",
  "main": "src/main_process/index.js",
  "scripts": {
    "build:prd": "npm run clean:www && NODE_ENV=prd webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "clean:www": "rm -rf www",
    "test": "karma start",
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=dev webpack-dev-server --config webpack.config.js --inline --progress --profile --colors --hot --display-error-details --display-cached --content-base src/",
    "start:stg": "",
    "start:prd": "",
    "start:adr:dev": "",
    "start:adr:stg": "",
    "start:adr:prd": "",
    "start:elt:dev": "NODE_ENV=dev ./node_modules/gulp/bin/gulp.js start:elt",
    "start:elt:stg": "",
    "start:elt:prd": "",
    "start:ios:dev": "",
    "start:ios:stg": "",
    "start:ios:prd": "",
    "start:win:dev": "",
    "start:win:stg": "",
    "start:win:prd": ""
  },
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">= 5.11.0",
    "npm": ">= 3.8.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "core-js": "2.4.0",
    "ionic-angular": "2.0.0-beta.8",
    "ionicons": "3.0.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "@angularclass/angular2-beta-to-rc-alias": "0.0.3",
    "angular2-hmr": "0.7.0",
    "awesome-typescript-loader": "0.19.0",
    "codelyzer": "0.0.23",
    "copy-webpack-plugin": "3.0.1",
    "cordova-icon": "^0.7.0",
    "cordova-splash": "^0.7.1",
    "electron-connect": "0.4.2",
    "electron-prebuilt": "^1.2.2",
