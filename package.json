    "ts-node": "0.9.0",
    "tslint": "3.11.0",
    "tslint-loader": "2.1.4",
    "typedoc": "0.4.2",
    "typescript": "1.8.10",
    "typings": "1.0.5",
    "webpack": "1.13.1",
